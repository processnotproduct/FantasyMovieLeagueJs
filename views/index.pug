extends layout

block content
  table
    thead
      th Rank
      th Name
      th % Used
      th $/Share
      th Total
    tbody
      each player in standings
        tr
          td: span= player.rank
          td 
            span= player.name
              if (player.bonus1)
                sup 1
              if (player.bonus2)
                sup 2
          td: span= player.sharesUsed
          td: span= player.perShareDisp
          td: span= player.totalDisp